--bubble gun

function bubblegunmove()
 if player.flip then
  if player.swmx then
   bubblegun.x=player.x-5
  else
   bubblegun.x=player.x-4
  end
  bubblegun.flip=true 
 else
  if player.swmx then
   bubblegun.x=player.x+5
  else
   bubblegun.x=player.x+4
  end
  bubblegun.flip=false 
 end
 bubblegun.y=player.y+1
end

function shootbubblegun()
 if btn(❎) then
  if bubble.stage=="shot" or bubble.stage=="ready" then
   bubblegun.charge+=1
   bubblegun.charging=true
   bubble.stage="ready"
   bubble.bouancy=0
   bubblegun.shot=false
  
   if bubblegun.charge<15 then
    bubble.size=0
   end
   if bubblegun.charge>15 then
    bubble.size=1
   end
   if bubblegun.charge>30 then
    bubble.size=2
   end
   if bubblegun.charge>45 then
    bubble.size=3
   end
   if bubblegun.charge>60 then
    bubble.size=4
   end
  end
 else
  if bubblegun.charging==true then
   bubblegun.charging=false  
  end  
  bubblegun.charge=0
 end
end

function drawbubble()
 
 --set bubble sprite
 if bubble.size==0 then
  bubble.sprite=0
  bubble.speed=0
 elseif bubble.size==1 then
  bubble.sprite=47
  bubble.speed=0.7
 elseif bubble.size==2 then
  bubble.sprite=31
  bubble.speed=0.6
 elseif bubble.size==3 then
  bubble.sprite=15
  bubble.speed=0.5
 elseif bubble.size==4 then
  bubble.sprite=1
  bubble.speed=0.4
 end
 
 if bubble.stage=="ready" then
  --set bubble sprite location
  bubble.y=bubblegun.y
  
  --sets bubble x
  if bubblegun.flip then
   bubble.x=bubblegun.x-6
  else
   bubble.x=bubblegun.x+6
  end
 end
 
 if bubble.stage=="shot" then
  bubble.sprite=112
 end
 
 spr(bubble.sprite,bubble.x,bubble.y)
 
end

function shootbubble()
 if bubble.size==0 then
  bubble.stage="shot"
 end
 
 if bubble.stage=="ready" or bubble.stage=="shooting" then
  if bubblegun.charge==0 then
   bubble.stage="shooting"
   if bubble.stage=="shooting" then
    if not bubblegun.shot then
     bubblegun.shot=true
     player.oxygen-=bubble.oxygencost
    end
    --apply drag/bouancy
    if bubble.bouancy<bubble.maxbouancy then
     bubble.bouancy+=bubble.bincrease
     if bubble.speed>0 then
      bubble.speed-=bubble.drag
     end
    end
    
    --move bubble
    if bubble.x>bubblegun.x and bubble.x<148 then
     bubble.x+=bubble.speed
     bubble.y-=bubble.bouancy
    elseif bubble.x<bubblegun.x and bubble.x>-20 then
     bubble.x-=bubble.speed
     bubble.y-=bubble.bouancy
    end
    
    if bubble.y<0 then
     bubble.stage="shot"
    end
     
    if bubble.x>138 or bubble.x+10<0 then
     bubble.speed=0
     bubble.stage="shot"
    end
   end
  end
 end
end
