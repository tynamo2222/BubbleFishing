function spawningstart()
	for i=1, 17 do
		addmovingsprite(i*8-17, 16+spawnmody, 0.05, 150, 0, flr(rnd(4)+2))
	end
	for i=1, 17 do
		addmovingsprite(i*8-17, 408+spawnmody, 0.05, 150, 0, flr(rnd(1.99)+10))
	end
	for i=1, 17 do
		addmovingsprite(i*8-17, 784+spawnmody, 0.05, 150, 0, flr(rnd(1.99)+16))
	end
end

wavex = {}
wavey = {}
waves = {}

function spawnwaves()
	for i=0, 17 do
		for a=1, 3 do
			add(wavey, flr(rnd(376)+56))
			add(wavex, i*8-8)
			add(waves, flr(rnd(1.99))+8)
		end
	end
end

function movewaves()
	for i=1, #wavex do
		spr(waves[i], wavex[i], wavey[i])
	end
	if wait(30) then
		if spriteposrl then
			for i=1, #wavex do
				wavex[i] += 1
			end
			spriteposrl = false
		else
			for i=1, #wavex do
				wavex[i] -= 1
			end
			spriteposrl = true
		end
	end	
end

function mediummovingwavestran()
	randmed = rnd(2)
	if randmed < 1 then
		addmovingsprite(-8, 408+spawnmody, 0.05, 150, 0, 10)
	else
		addmovingsprite(-8, 408+spawnmody, 0.05, 150, 0, 11)
	end
end

function darkmovingwavestran()
	randdark = rnd(2)
	if randdark < 1 then
		addmovingsprite(-8, 784+spawnmody, 0.05, 150, 0, 16)
	else
		addmovingsprite(-8, 784+spawnmody, 0.05, 150, 0, 17)
	end
end

function lightmovingwavestran()

	randlight = rnd(5)
	if randlight <= 1 then
		addmovingsprite(-8, 16+spawnmody, 0.05, 150, 0, 2)
	elseif randlight <=2 and randlight >1 then
		addmovingsprite(-8, 16+spawnmody, 0.05, 150, 0, 3)		
	elseif randlight <=3 and randlight >2 then
		addmovingsprite(-8, 16+spawnmody, 0.05, 150, 0, 4)
	elseif randlight <=4 and randlight >3 then
		addmovingsprite(-8, 16+spawnmody, 0.05, 150, 0, 5)
	else
		addmovingsprite(-8, 16+spawnmody, 0.05, 150, 0, 6)
	end
end



