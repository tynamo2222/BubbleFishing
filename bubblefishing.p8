--player

function collidewithplayer(x,y)
 if x>=player.x+1 and x<=player.x+7 then
  if y>=player.y and y<=player.y+8 then
   return true
  else
   return false
  end
 end
end

function drawplayer(x, y)
	spr(player.sprite, x,	 y, 1, 1, player.flip, false)
end

function moveplayer()
	 --physics. slows down movement
  player.y+=gravity
  player.yspd*=friction
  
  --[[friction is less then 1,
  multipliying by a negative
  decreases]]
  player.xspd*=friction
  
  --makes x/y stable 
  if player.xspd<0.005 and player.xspd>0 then
   player.xspd=0
  elseif player.yspd<0.005 and player.yspd>0 then
   player.yspd=0
  elseif player.yspd<0 and player.yspd>0.005 then
   player.yspd=0
  elseif player.xspd<0 and player.xspd>0.005 then
   player.xspd=0
  end
  
  if btn(➡️) then
   player.xspd+=player.xacc
   player.flip=false
   player.swmx=true
  end
 	if btn(⬅️) then
   player.xspd-=player.xacc
   player.flip=true
   player.swmx=true
  end
  if btn(⬇️) then
   player.yspd+=player.yacc
   mapy -= 1
   spawnmody -=1
   for i = 1, #movespry do
   	movespry[i] -=1
   end
   for i = 1, #wavey do
   	wavey[i] -=1
   end
   if not btn(➡️) and not btn(⬅️) then
    player.swmx=false
    player.swmu=false
   end
  end
  if btn(⬆️) then
   player.yspd-=player.yacc
   mapy += 1
   player.swmu=true
   player.swmx=false
   spawnmody +=1
   for i = 1, #movespry do
   	movespry[i] +=1
   end
   for i = 1, #wavey do
   	wavey[i] +=1
   end
  elseif not btn(⬇️) and not btn(➡️) and not btn(⬅️) then
   player.swmx=false
   player.swmu=false
  end
  
  player.x+=player.xspd
  player.y+=player.yspd
end

function animateplayer()
 if player.swmx==false and player.swmu==false then
  player.sprite=53
 end
 player.animate+=1
 if player.animate>=20 then
  player.animate=0
 end
 if player.animate>10 then
  if player.swmx then
   player.sprite=54
  elseif player.swmu then
   player.sprite=56
  end
 elseif player.animate<10 then
  if player.swmx then
   player.sprite=55
  elseif player.swmu then
   player.sprite=53
  end
 end
end
