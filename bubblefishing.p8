function menustuff()
 
  if titlecount>=60 then
	  titlecount=0
	 end
	 
	 titlecount+=1
	 
	 if titlecount>30 then
	  titlecolor=7
	 else
	  titlecolor=6
	 end
	 
	 if titlecount<15 or titlecount>45 then
	  titley=23
	 else
	  titley=24
	 end
	 
	 if btnp(❎) and gamestate=="menu" then
	  transition=true
	 end
	
	
	if transition==true then
	 if transitionx<128 or transitionx>256 then
	  transitionx+=10
	 else
   transitionx+=5	 
	 end
	 
	 if transitionx>=356 then
	  transitionx=0
	  transition=false
	 end
	 
	 if transitionx>=200 then
	  gamestate="run"
	 end
	end
end

