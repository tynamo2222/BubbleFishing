function drawmap(x, y)	
	xpos = x
	ypos = y
	for i = 1, #mapindex do
		for a = 1, #mapindex[i] do
			spr(mapindex[i][a],xpos, ypos)
			xpos += 8
		end
		xpos = x
		ypos +=8
	end
end 

